{
  "extends": ["next/core-web-vitals", "next/typescript", 
  "prettier"
  ],
"plugins": ["prettier"],
  "rules": {
    "arrow-body-style": "off",
    "prettier/prettier": "error",
    "no-console": "error",
    "max-len": ["error", { "code": 85, "tabWidth": 4, "ignorePattern": "^\\s*\\w+" }],
    "prefer-arrow-callback": "off"
  }
}
